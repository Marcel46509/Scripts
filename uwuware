end)

Aimbot:AddSlider("Silent Aim FOV", 1, 1, 1000, 1, function(Value : number)
	Configuration.SilentFOV = Value
end)

Aimbot:AddSlider("Smoothness", 1, 1, 10, 1, function(Value : number)
	Configuration.Smoothness = Value
end)

